@model IEnumerable<Model.Permission>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PermissionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ControllName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActionType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PermissionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ControllName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActionType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PermissionId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PermissionId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PermissionId })
        </td>
    </tr>
}

</table>
