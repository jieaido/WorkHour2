
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/DateTimerPicker/js/bootstrap-datetimepicker.js"></script>
    <link href="~/Scripts/DateTimerPicker/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <script src="~/Scripts/DateTimerPicker/js/locales/bootstrap-datetimepicker.zh-CN.js"></script>
    <title>register</title>
</head>
<body>
    <div class="container">

        <form class="login-form"  method="POST" style="max-width: 330px;margin:auto">
            <h2 class="form-signin-heading">Please sign in</h2>
            <label for="inputEmail" class="sr-only">Email address</label>
            <input name="mname" type="email" id="inputEmail" class="form-control" placeholder="Email address" required autofocus>

            <label for="inputPassword" class="sr-only">Password</label>
            <input type="password" name="mpwd" style="margin-top:20px" id="inputPassword" class="form-control" placeholder="Password" required>
            <div class="form-group">
            
            @Html.DropDownList("teamid", ViewBag.selectlist as SelectList, new { @class = "form-control",style="margin-top:20px" } );
            </div>
                <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
        </form>

    </div> <!-- /container -->
</body>
</html>
